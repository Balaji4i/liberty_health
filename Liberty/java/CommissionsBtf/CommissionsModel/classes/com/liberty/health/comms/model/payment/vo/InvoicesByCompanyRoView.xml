      Type="oracle.jbo.domain.Number"/>
    <SQLQuery><![CDATA[select i.company_id_no, c.company_name, i.invoice_no,   
       invoice_date,   
       PAYMENT_RECEIVE_DATE contribution_date,   
       country_code,   
       release_date,   
       release_hold_reason,   
       sum(id.fee_type_amt) invoie_amt , 
       sum(id.FEE_TYPE_EXCH_AMT) exch_invoie_amt  
from invoice i, invoice_detail id, company c   
where release_date is null   
  and i.invoice_no = id.invoice_no   
  and i.company_id_no = c.company_id_no   
group by i.company_id_no, c.company_name, i.invoice_no,   
       invoice_date,   
       PAYMENT_RECEIVE_DATE,   
       country_code,   
       release_date,   
       release_hold_reason]]></SQLQuery>
