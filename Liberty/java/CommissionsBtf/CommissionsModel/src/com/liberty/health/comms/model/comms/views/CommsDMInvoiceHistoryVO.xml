  <SQLQuery><![CDATA[SELECT INV.COUNTRY_CODE   
        ,INV.COMPANY_ID_NO   
        ,INVT.INVOICE_TYPE_DESC   
        ,INV.INVOICE_NO INVOICE_NUMBER   
        ,INV.INVOICE_DATE   
        ,INV.PAYMENT_RECEIVE_DATE   
        ,INV.RELEASE_DATE   
        ,INV.RELEASE_HOLD_REASON   
        ,INV.PAYMENT_DATE   
        ,INV.PAYMENT_REJECT_DATE   
        ,INV.PAYMENT_REJECT_DESC   
        ,INV.PAYMENT_AMT INVOICE_PAYMENT_AMT   
        ,INV.PAYMENT_EXCH_RATE   
        ,INV.CURRENCY_CODE   
        ,INV.LAST_UPDATE_DATETIME   
        ,INV.USERNAME   
        ,INV.INVOICE_TAX_CODES   
        ,INVD.FEE_TYPE_AMT      INV_DETAIL_AMT   
        ,INVD.FEE_TYPE_EXCH_AMT INV_DETAIL_EXCH_AMT   
        ,INVD.FEE_TYPE_DESC     INV_DETAIL_DESCRIPTION   
FROM INVOICE_DM        INV,   
     INVOICE_DETAIL_DM INVD,   
     INVOICE_TYPE      INVT   
WHERE INV.INVOICE_NO  = INVD.INVOICE_NO   
  AND INV.INVOICE_TYPE_ID_NO  = INVT.INVOICE_TYPE_ID_NO
  AND INV.COUNTRY_CODE IN (SELECT regexp_substr(replace( :pCountryList ,  '''' , '')     
 , '[^,]+', 1, LEVEL)             
            FROM DUAL             
            CONNECT BY regexp_substr( replace( :pCountryList ,  '''' , '')   
, '[^,]+', 1, LEVEL) IS NOT NULL)]]></SQLQuery>
