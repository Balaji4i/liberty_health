    <SQLQuery><![CDATA[select b.broker_id_no, b.broker_name, b.broker_last_name, person_title_code, company_id_no,     
       b_status.broker_table_type_desc status_desc,    
       b.effective_start_date,    
       b.effective_end_date    
from broker b, broker_function bf, broker_table_type btt,    
 (select bf.broker_id_no, btt.broker_table_type_desc             
         from broker_function bf, broker_table bt, broker_table_type btt             
        where bf.broker_table_id_no = bt.broker_table_id_no             
          and upper(broker_table_desc) = 'STATUS'             
          and bf.broker_table_type_id_no = btt.broker_table_type_id_no             
          and trunc(sysdate) between bf.effective_start_date and bf.effective_end_date) b_status    
where b.broker_id_no = bf.broker_id_no     
  and trunc(sysdate) between bf.effective_start_date and bf.effective_end_date     
  and bf.broker_table_id_no = btt.broker_table_id_no     
  and bf.broker_table_type_id_no = btt.broker_table_type_id_no     
  and upper(btt.broker_table_type_desc) like '%CONSULTANT%'    
  and b.broker_id_no = b_status.broker_id_no(+)]]></SQLQuery>
