<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
    <task-flow-definition id="welcome-btf">
        <default-activity>welcome</default-activity>
        <data-control-scope>
            <shared/>
        </data-control-scope>
        <task-flow-reentry>
            <reentry-not-allowed/>
        </task-flow-reentry>
        <managed-bean id="__1">
            <managed-bean-name>dashboardBean</managed-bean-name>
            <managed-bean-class>com.liberty.health.comms.home.view.beans.Dashboard</managed-bean-class>
            <managed-bean-scope>session</managed-bean-scope>
        </managed-bean>
        <view id="welcome">
            <page>/WEB-INF/fragments/welcome.jsff</page>
        </view>
        <use-page-fragments/>
    </task-flow-definition>
</adfc-config>
