<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="brokerage-payments-adjustments">
    <default-activity>brokeragePaymentsAdjustments</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>paymentRunBean</managed-bean-name>
      <managed-bean-class>com.liberty.health.comms.btf.beans.PaymentRun</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__2">
      <managed-bean-name>Payments</managed-bean-name>
      <managed-bean-class>com.liberty.health.comm.vc.view.beans.Payments</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <view id="brokeragePaymentsAdjustments">
      <page>/WEB-INF/fragments/comms/brokeragePaymentsAdjustments.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
